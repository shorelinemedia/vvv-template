{
    "name": "shorelinemedia/vvv-template",
    "description": "A VVV-hosted WordPress site managed by Composer.",
    "type": "project",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Shoreline Media Marketing",
            "email": "team@shoreline.media"
        }
    ],
    "repositories": [
      {
          "type": "composer",
          "url": "https://wpackagist.org"
      },
      {
          "type": "composer",
          "url": "https://premium.wpmudev.org/"
      },
      {
          "type": "package",
          "package": {
              "name": "afragen/github-updater",
              "type": "wordpress-plugin",
              "version": "0",
              "require": {
                  "composer/installers": "^1"
              }
          }
      }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "wpackagist-plugin/akismet": ">=3.3",
        "wpackagist-plugin/wordpress-seo": ">=11.0",
        "wpackagist-plugin/mailchimp-for-wp": ">=4.5",
        "wpackagist-plugin/members": ">=2.1",
        "wpackagist-plugin/contactic": ">=1.4.0",
        "wpackagist-plugin/disable-gutenberg": "^2.0",
        "afragen/github-updater": "^9.3"
    },
    "require-dev": {
        "wpackagist-plugin/log-deprecated-notices": ">=0.4.0",
        "wpackagist-plugin/debug-bar": ">=0.9.0",
        "wpackagist-plugin/genesis-visual-hook-guide": ">=1.0",
        "wpackagist-plugin/debug-bar-extender": ">=0.5",
        "wpackagist-plugin/piglatin": ">=0.2",
        "wpackagist-plugin/query-monitor": ">=2.7",
        "wpackagist-plugin/rewrite-rules-inspector": ">=1.2",
        "wpackagist-plugin/user-switching": ">=1.0",
        "wpackagist-plugin/what-the-file": ">=1.4",
        "wpackagist-plugin/wp-crontrol": ">=1.2",
        "wpackagist-plugin/gravityformscli": ">=1.0",
        "wpackagist-plugin/widget-importer-exporter": ">=1.5"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}/": [
                "goblindegook/syllables"
            ],
            "public_html/wp-content/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "public_html/wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "public_html/wp-content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "public_html"
    }
}
